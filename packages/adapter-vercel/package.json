{
  "name": "@uix/adapter-vercel",
  "version": "0.0.1",
  "description": "Adapter to convert Vercel AI SDK messages to UIX Lucid IR format",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@uix/core": "workspace:*"
  },
  "peerDependencies": {
    "ai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "ai": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "uix",
    "vercel",
    "ai-sdk",
    "adapter",
    "converter"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/UIX.git",
    "directory": "packages/adapter-vercel"
  }
}
